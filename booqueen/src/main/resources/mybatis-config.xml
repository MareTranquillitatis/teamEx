<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<!-- user -->
		<typeAlias type="com.booqueen.user.member.MemberVO" alias="userMemberVO"/>
		<typeAlias type="com.booqueen.user.review.vo.ReviewVO" alias="reviewVO"/>
		<typeAlias type="com.booqueen.user.review.vo.ReviewAvgVO" alias="reviewAvgVO"/>
		<typeAlias type="com.booqueen.user.wishlist.vo.WishlistVO" alias="wishlistVO"/>
		<typeAlias type="com.booqueen.user.member.vo.MemberProfileVO" alias="memberProfileVO"/>
		<typeAlias type="com.booqueen.user.review.vo.ReviewProfileVO" alias="reviewProfileVO"/>
		<typeAlias type="com.booqueen.user.room.vo.RoomVO" alias="roomVO"/>
		<typeAlias type="com.booqueen.user.room.vo.RoomFacilitiesVO" alias="roomFacilitiesVO"/>
		<typeAlias type="com.booqueen.user.chat.vo.ChatVO" alias="chatVO"/>
		
		<!-- shared -->
		<typeAlias type="com.booqueen.user.hotel.vo.HotelVO" alias="hotelVO"/>
		<typeAlias type="com.booqueen.user.hotel.vo.HotelImgVO" alias="hotelImgVO"/>
		
		<!-- partner -->
  		<typeAlias type="com.booqueen.partner.member.MemberVO" alias="myMember"/>
	</typeAliases>
	<mappers>
	  <mapper resource="mappings/user/member-mapping.xml"/>
	  <mapper resource="mappings/user/profile-mapping.xml"/>
	  <mapper resource="mappings/user/hotel-mapping.xml"/>
	  <mapper resource="mappings/user/review-mapping.xml"/>
	  <mapper resource="mappings/user/wishlist-mapping.xml"/>
	  <mapper resource="mappings/user/room-mapping.xml"/>
	  <mapper resource="mappings/user/chat-mapping.xml"/>
	  <mapper resource="mappings/member-mapping.xml"/>
	</mappers>
</configuration>